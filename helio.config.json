{
  "apiKey": "${HELIO_API_KEY}",
  "apiUrl": "${HELIO_API_URL}",
  "network": "mainnet",
  "rateLimit": {
    "requestsPerSecond": 30,
    "maxRetries": 3
  },
  "deployment": {
    "instances": {
      "min": 1,
      "max": 3
    },
    "healthCheck": {
      "interval": 30,
      "endpoint": "/api/health"
    }
  },
  "logging": {
    "level": "info",
    "format": "json"
  },
  "errorHandling": {
    "retryAttempts": 3,
    "backoffFactor": 1.5
  }
}